box-shadow() 
    -moz-box-shadow arguments
    -webkit-box-shadow arguments
    -ms-box-shadow arguments
    box-shadow arguments

box-shadow2($arguments,$second) 
    -moz-box-shadow: $arguments, $second
    -webkit-box-shadow: $arguments, $second
    -ms-box-shadow: $arguments, $second
    box-shadow: $arguments, $second

box-shadow3($arguments,$second,$third) 
    -moz-box-shadow: $arguments,$second,$third
    -webkit-box-shadow: $arguments,$second,$third
    -ms-box-shadow: $arguments,$second,$third
    box-shadow: $arguments,$second,$third

lineargradient3points($color,$color2,$color3)
    background: $color
    background: -moz-linear-gradient(top, $color 0%, $color2 50%, $color3 100%) /* FF3.6-15 */
    background: -webkit-linear-gradient(top, $color 0%,$color2 46%,$color3 100%) /* Chrome10-25,Safari5.1-6 */
    background: linear-gradient(top, $color 0%,$color2 46%,$color3 100%) /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */    

border-radius($radius)
    -moz-border-radius: $radius
    -webkit-border-radius: $radius
    border-radius: $radius